{
  "title": "Self-Play Health",
  "uid": "selfplay-health",
  "timezone": "browser",
  "schemaVersion": 38, "version": 1,
  "panels": [
    {"type":"stat","title":"Games/min","targets":[{"expr":"sum(rate(chs_selfplay_games_total[1m]))"}]},
    {"type":"gauge","title":"Win-Rate (15m)","targets":[{"expr":"100 * (sum(increase(chs_selfplay_wins_total[15m])) / sum(increase(chs_selfplay_games_total[15m])))"}]},
    {"type":"stat","title":"Failures/min","targets":[{"expr":"sum(rate(chs_selfplay_failures_total[5m]))"}]},
    {"type":"table","title":"Runs (letzte 1h)","targets":[{"expr":"sum by (run_id) (increase(chs_selfplay_games_total[1h]))"}]}
  ]
}
