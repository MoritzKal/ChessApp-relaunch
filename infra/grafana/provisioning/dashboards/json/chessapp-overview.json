{
  "id": null,
  "uid": "chs-overview",
  "title": "ChessApp â€“ Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "links": [
    {
      "title": "Predict Logs",
      "url": "/explore?left=[\"now-1h\",\"now\",\"loki\",{\"expr\":\"{component=\\\"serve\\\"} | json | event=~\\\"predict.*\\\"\"}]"
    }
  ],
  "panels": [
    {
      "type": "timeseries",
      "title": "Ingest Runs (chs_ingest_runs_started_total)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "chs_ingest_runs_started_total",
          "legendFormat": "{{application}}/{{component}} ({{username}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Dataset Builds (chs_dataset_build_total)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "chs_dataset_build_total",
          "legendFormat": "{{application}}/{{component}} ({{username}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Dataset Rows (chs_dataset_rows)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "chs_dataset_rows",
          "legendFormat": "{{application}}/{{component}} ({{username}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Active Ingest Runs (chs_ingest_active_runs)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "chs_ingest_active_runs",
          "legendFormat": "{{application}}/{{component}} ({{username}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "logs",
      "title": "API Logs (Loki)",
      "gridPos": { "x": 0, "y": 45, "w": 24, "h": 12 },
      "options": {
        "showLabels": true,
        "showTime": true,
        "wrapLogMessage": true,
        "enableLogDetails": true
      },
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "{service=\"api\"} | json",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Training Loss (chs_training_loss)",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "chs_training_loss" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Val Accuracy (chs_training_val_accuracy)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "chs_training_val_accuracy" }
      ]
    },
    {
      "type": "stat",
      "title": "Runs total (chs_training_runs_total)",
      "gridPos": { "x": 0, "y": 24, "w": 6, "h": 5 },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "prometheus" }, "expr": "sum(chs_training_runs_total)" }
      ]
    },
    {
      "uid": "serveReq",
      "type": "timeseries",
      "title": "Serve Requests/min",
      "gridPos": { "x": 0, "y": 29, "w": 8, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(chs_predict_requests_total[1m]))",
          "legendFormat": "total",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveLat",
      "type": "heatmap",
      "title": "Predict Latency",
      "gridPos": { "x": 8, "y": 29, "w": 8, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(chs_predict_latency_seconds_bucket[1m])) by (le)",
          "legendFormat": "{{le}}",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveIll",
      "type": "stat",
      "title": "Illegal Requests (sum)",
      "gridPos": { "x": 16, "y": 29, "w": 8, "h": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(chs_predict_illegal_requests_total)",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveReqVer",
      "type": "timeseries",
      "title": "Predict Requests/min (by version)",
      "gridPos": { "x": 0, "y": 37, "w": 6, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (model_id,model_version) (rate(chs_predict_requests_total{job=\\\"serve\\\"}[1m]))",
          "legendFormat": "{{model_id}}/{{model_version}}",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveHist95",
      "type": "timeseries",
      "title": "Predict p95 Latency (hist)",
      "gridPos": { "x": 6, "y": 37, "w": 6, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le,model_id,model_version) (rate(chs_predict_latency_seconds_bucket{job=\\\"serve\\\"}[5m])))",
          "legendFormat": "{{model_id}}/{{model_version}}",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveSummary95",
      "type": "timeseries",
      "title": "Predict p95 Latency (ms summary)",
      "gridPos": { "x": 12, "y": 37, "w": 6, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "max by (model_id,model_version) (chs_predict_latency_ms{quantile=\\\"0.95\\\",job=\\\"serve\\\"})",
          "legendFormat": "{{model_id}}/{{model_version}}",
          "refId": "A"
        }
      ]
    },
    {
      "uid": "serveErrIll",
      "type": "timeseries",
      "title": "Errors & Illegal/min",
      "gridPos": { "x": 18, "y": 37, "w": 6, "h": 8 },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "stacking": { "mode": "normal" } },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (model_id,model_version) (rate(chs_predict_errors_total{job=\\\"serve\\\"}[1m]))",
          "legendFormat": "errors {{model_id}}/{{model_version}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (model_id,model_version) (rate(chs_predict_illegal_requests_total{job=\\\"serve\\\"}[1m]))",
          "legendFormat": "illegal {{model_id}}/{{model_version}}",
          "refId": "B"
        }
      ]
    },
    {
      "type": "text",
      "title": "Predict by Version \u2192",
      "gridPos": { "x": 0, "y": 37, "w": 6, "h": 4 },
      "options": { "content": "[Predict by Version \u2192](/d/predict-by-version)" }
    }
  ]
}
