{
  "title": "Dataset Quality",
  "uid": "ds-quality-v0",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Invalid Rate (%)",
      "targets": [
        {
          "expr": "sum(chs_dataset_invalid_rows_total) / sum(chs_dataset_rows_total) * 100"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Rows Processed",
      "targets": [
        {
          "expr": "sum(rate(chs_dataset_rows_total[5m]))"
        }
      ]
    },
    {
      "type": "barchart",
      "title": "Invalid by Reason",
      "targets": [
        {
          "expr": "sum by (reason) (increase(chs_dataset_invalid_rows_total[1h]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Last Export Duration (ms)",
      "targets": [
        {
          "expr": "sum(chs_dataset_export_duration_ms_sum) / sum(chs_dataset_export_duration_ms_count)"
        }
      ]
    }
  ]
}